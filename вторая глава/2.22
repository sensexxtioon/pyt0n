import math; 
print("Введите первое число"); 
x1 = int(input()); 
print("Введите второе число"); 
x2 = int(input()); 
a1 = (abs(x1)+abs(x2))/2; 
print("Среднее арифметическое модулей этих двух чисел равно", a1); 
g = math.sqrt(abs(x1)*abs(x2)); 
print("Среднее геометрическое модулей этих двух чисел равно", g);
